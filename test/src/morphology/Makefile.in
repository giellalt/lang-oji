# Add language-specific flags for hfst-lexc compilation here:
HFST_LEXC_LOCAL_FLAGS = 

# Set this to name of lexc file containing Multichar_Symbols and LEXICON Root
GT_LEXC_ROOT = root.lexc

# Set this to the names of all regular lexc source files:
GT_LEXC_SRCS_L1_L2 = \
			 affixes/nouns.lexc       \
			 affixes/verbs.lexc       \
			 stems/nouns.lexc         \
			 stems/numerals.lexc      \
			 stems/pronouns.lexc      \
			 stems/punctuation.lexc   \
			 stems/verbs.lexc


#			 affixes/propernouns.lexc \
# If you are building an error-detecting L2 analyser, specify the lexc files
# that differ between the regular L1 and the L2 analysers below, in L1 and
# L2 respectively. L2 files must end in "*-L2.lexc". See SME for an example.
L1 = 
L2 = 
GT_LEXC_SRCS = \
			$(GT_LEXC_SRCS_L1_L2) \
			$(L1)

GT_LEXC_L2_SRCS = \
			$(GT_LEXC_SRCS_L1_L2) \
			$(L2)


# Set this to the names of all generated lexc files, if any
GENERATED_LEXC_SRCS = 

# Set this to the names of all source xml files, if any
GT_XML_SRCS = 

# Define any additional sources here (compiled on their own):
GT_LOCAL_SRCS = 

# Define here any additional sources just included in the distro:
GT_DISTRO_SRCS = 
HFST_LEXC_FLAGS = $(HFST_LEXC_LOCAL_FLAGS) $(am__append_1) \
	$(am__append_2)

####### Source file defs: ########

#! @param GT_LEXC_ROOT required, define name of file holding root lexicon
#! @param GT_LEXC_SRCS optional, space separated list of file names holding
#!		  supplementary lexicon data
#! @param GENERATED_LEXC_SRCS optional, space separated list of file names
#!        holding supplementary lexicon data generated from xml files.
#! @param GT_XML_SRCS optional, space separated list of xml source file names
GT_LEXC_ALLSRC = $(GT_LEXC_ROOT) $(GT_LEXC_SRCS_L1_L2) $(L1) $(L2) \
				$(GENERATED_LEXC_SRCS) $(GT_XML_SRCS) $(GT_LOCAL_SRCS) \
				$(GT_DISTRO_SRCS)


# All sources need to be included in the tarball
EXTRA_DIST = $(GT_LEXC_ALLSRC)

